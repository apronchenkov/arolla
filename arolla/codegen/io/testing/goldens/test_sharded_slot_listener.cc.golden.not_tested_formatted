std::vector<std::unique_ptr<::arolla::SlotListener<::testing_namespace::Root>>>
GetShardedProtoSizedSlotListener_Shards() {
  std::vector<
      std::unique_ptr<::arolla::SlotListener<::testing_namespace::Root>>>
      res;
  res.emplace_back() = MakeNotOwningSlotListener(
      ::my_namespace_GetShardedProtoSizedSlotListenerImplNamespace::
          GetCollectionSlotListener());
  res.emplace_back() = MakeNotOwningSlotListener(
      ::my_namespace_GetShardedProtoSizedSlotListenerImplNamespace::shard1::
          GetCollectionSlotListener());
  return res;
}
}  // namespace

std::unique_ptr<::arolla::SlotListener<::testing_namespace::Root>>
GetShardedProtoSizedSlotListener() {
  static const SlotListener<
      ::my_namespace_GetShardedProtoSizedSlotListenerImplNamespace::Output>*
      impl = ChainSlotListener<
                 ::my_namespace_GetShardedProtoSizedSlotListenerImplNamespace::
                     Output>::Build(GetShardedProtoSizedSlotListener_Shards())
                 .value()
                 .release();
  return ::arolla::MakeNotOwningSlotListener(impl);
}

